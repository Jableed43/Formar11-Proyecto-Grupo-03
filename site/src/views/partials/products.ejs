<div class="row">

    <% products.map(products => { %>
            
    <div class="contenedor">
        <section class="products">
            <a href="/products/detail/<%= products.id %> ">
                <article class="product-container">
                    <img src="/images/imgmenu/<%= products.images %>" alt= "Foto Producto">
                    <p><%= products.title %></p>
                    <nav>

                        <ul type="none">

                            <li><%= products.description %></li>

                        </ul>
                    </nav>
                        <div class="botonera-contenedor">
                        <button class="ver-mas">Ver mÃ¡s</button>
                        <button class="plus" onclick="addItem(event, '<%= products.id %>')">+</button>
                        </div>
                </article>
            </a>
        </section>
    </div>

<% }) %>
</div>